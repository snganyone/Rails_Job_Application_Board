<% if job.errors.any? %>
<div id="error_explanataion">
    <h2>There were some errors: </h2>
    <ul>
        <% job.errors.full_messages.each do |message| %>
            <li><%= message %></li>
        <% end %>
    </ul>
</div>
<% end %>


<%= form_for job do |f| %>
    <%= f.collection_select :user_id, User.all, :id, :name %>
    <br><br><br>
    <%= f.collection_select :agency_id, Agency.all, :id, :name %>
    <br><br><br>

    <%= f.label :title %>
    <%= f.text_field :title %>
    <br><br>

    <%= f.label :employer %>
    <%= f.text_field :employer %>
    <br><br>

    <%= f.label :location %>
    <%= f.text_field :location %>
    <br><br>

    <%= f.label :description %>
    <%= f.text_field :description %>
    <br><br>

    <%= f.label :release_date %>
    <%= f.date_field :release_date %>
    <br><br>

    <%= f.label :job_type %>
    <%= f.text_field :job_type %>
    <br><br>

    <%= f.submit "Submit" %>

<% end %>