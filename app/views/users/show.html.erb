<p><%= @user.name %></p>

<h3>Your Job Postings</h3>

<%= link_to "Post a new Job", new_job_path %>

<ul>
<% current_user.jobs.each do |j| %>
    <li><%= j.title %></li>
    <li><%= j.employer %></li>
    <li><%= j.location %></li>
    <li><%= j.description %></li>
    <li><%= j.release_date %></li>
    <li><%= j.job_type %></li>

    <%= link_to "Edit Job Post", edit_job_path(j) %>
    <br>
    <br>
<% end %>
</ul>