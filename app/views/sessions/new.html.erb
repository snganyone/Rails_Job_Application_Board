<h1>Sign In</h1>

<%= form_for :user do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    
    <%= f.submit "Sign In" %>
<% end %>

<br><br>

<%= button_to "Log in with Google", '/auth/google_oauth2' %>